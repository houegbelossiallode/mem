{% extends 'template.html.twig' %}

{% block title %}Hello UsersController!{% endblock %}

{% block body %}


<section class="section">
      <div class="row">
        <div class="col-lg-12">

          <div class="card">
            <div class="card-body">
              <h5 class="card-title"></h5>


{{ form_start(form) }}
<div class="row">

<div class="col-4">{{ form_row(form.roles) }}</div>
<div class="col-4"> {{ form_row(form.nom) }} </div>
<div class="col-4"> {{ form_row(form.email) }} </div>
</div>
</div>
<button type="submit" class="btn btn-primary">VALIDER</button>

{{ form_rest(form) }}

{% endblock %}